                    { "element": "WiseMenuButton", "id" : "btnPrintMenu", "onclick": "btnPrintMenu_onClick",  "icon" : "/application-manager/images/print.png", "text" : "Print this list" },
                    { "element": "WiseMenuVerticalSeparator" },
