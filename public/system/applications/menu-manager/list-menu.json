                    { "element": "WiseDataTable", "id" : "tableListOfMenu", "onrowdblclick" : "tableListOfMenu_onRowDoubleClick",
                        "columns": [
                        { "datafield" : "icon", "text": "Icon", "type": "image", "width" : "60" },
                        { "datafield" : "title", "text": "Title", "width" : "200"},
                        { "datafield" : "description", "text": "Description" },
                        { "datafield" : "menuType", "text": "Type" },
                        { "datafield" : "parent.title", "text": "Parent Menu" },
                        { "datafield" : "appID", "text": "Application" },
                        { "datafield" : "appCommand", "text": "Command" }
                    ]}
                ]
