                    { "element": "WiseMenuButton", "id" : "btnEditMenu", "onclick": "btnEditMenu_onClick", "icon" : "/application-manager/images/edit.png", "text" : "Edit" },
                    { "element": "WiseMenuButton", "id" : "btnDeleteMenu", "onclick": "btnDeleteMenu_onClick", "icon" : "/application-manager/images/delete.png", "text" : "Remove" },
                    { "element": "WiseMenuButton", "id" : "btnCopyMenu", "onclick": "btnCopyMenu_onClick", "icon" : "/application-manager/images/copy.png", "text" : "Copy" },
                    { "element": "WiseMenuButton", "id" : "btnPasteMenu", "onclick": "btnPasteMenu_onClick", "icon" : "/application-manager/images/paste.png", "text" : "Paste" },
                    { "element": "WiseMenuVerticalSeparator" },
