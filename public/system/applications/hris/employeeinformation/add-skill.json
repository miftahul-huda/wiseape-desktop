{
    "root" :
    {
        "children" : 
        [
            {
                "element" : "WiseDiv",
                "children" : 
                [
                    { "element": "WiseTextBox", "id": "skillName", "data" : "skillName", "label" : "Skill", "placeholder" : "Enter the employee's skill" }
                ]
            }
            ,
            {
                "element" : "WiseDiv",
                "children" : 
                [
                    { "element": "WiseComboBox", "id": "cmbSkillLevel", "data" : "skillLevelCode", "lookupData":"skillLevel", "lookupValue":"code", "lookupText":"skillLevel", "label" : "Skill Level", "placeholder" : "Enter the employee's skill level" }
                ]
            }
            ,
            {
                "element" : "WiseDiv",
                "children" : 
                [
                    { "element": "WiseComboBox", "id": "cmbSkillGroup", "data" : "skillGroupCode", "lookupData":"skillGroup", "lookupValue":"code", "lookupText":"skillGroup", "label" : "Skill Group", "placeholder" : "Enter the employee's skill group" }
                ]
            }      
            ,
            {
                "element": "WiseDiv", "class":"vertical-separator"
            }
            ,
            {
                "element" : "WiseDiv", "class" : "flex-div",
                "children": 
                [
                    { "element": "WiseButton", "id" : "btnOk", "onclick": "btnOk_onClick", "icon" : "fa fa-check", "text" : "Ok" },
                    { "element": "WiseButton", "id" : "btnCancel", "onclick": "btnCancel_onClick", "icon" : "fa fa-ban", "text" : "Cancel" }
                ]
            }
        ]
    }
}